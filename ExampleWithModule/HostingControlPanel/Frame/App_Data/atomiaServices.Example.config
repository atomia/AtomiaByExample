        </atomiaServiceProperty>
      </atomiaServicePropertiesList>
      <atomiaServiceProvisioningDescriptionList>
        <atomiaServiceProvisioningDescription path="CsBase" serviceName="Example" provisionDescriptionID="ProvisioningDescriptionGlobal" />
